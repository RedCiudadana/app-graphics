<div class="darrow">
  <img src="/assets/img/down.png" />
</div>

<div class="dtext wow fadeInUp animated">
  <h3>Presupuestos municipales</h3>
</div>

<div class="three2 wow fadeInUp animated">
  <div class="three">
    <div class="col-lg-4 col-md-4 col-xs-4">
      <div class="timg">
        <img src="assets/img/t1.png" />
      </div>
    </div>
    <div class="col-lg-4 col-md-4 col-xs-4">
      <div class="timg">
        <img src="assets/img/t2.png" />
      </div>
    </div>
    <div class="col-lg-4 col-md-4 col-xs-4">
      <div class="timg">
        <img src="assets/img/t3.png" />
      </div>
    </div>
  </div>
</div>

<div class="after wow fadeInUp animated">
  <div class="col-lg-12">
    <label style="margin-right: 10px;">Municipalidad</label>
    {{#power-select
      selected=currentMunicipalidad
      options=model
      onchange=(action 'selectMunicipalidad')
      searchField='nombreMunicipio'
      placeholder='Buscar'
        as |municipalidad|
    }}
      <strong>{{municipalidad.nombreMunicipio}}</strong> <small>{{municipalidad.departamento}}</small>
    {{/power-select}}

    <div class="button">
      <button
        type="button"
        class="btn btn-default"
        {{action 'consultarMunicipalidad'}}
      >Consultar</button>
    </div>
  </div>
</div>
