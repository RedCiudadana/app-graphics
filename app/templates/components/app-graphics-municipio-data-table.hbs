{{#light-table table
  tableClassNames='table table-condensed'
  as |t|
}}
  {{t.head
    onColumnClick=(action 'onColumnClick')
    iconAscending='fa fa-sort-asc'
    iconDescending='fa fa-sort-desc'
    sortOnClick=true
  }}
  {{t.body canSelect=false classNames='table-responsive'}}
{{/light-table}}
